#include <stdio.h>
void swap(int *x,int *y)
{
    int t;
    t=*x;
    *x=*y;
    *y=t;
}
int main() {
int a,b;
printf("Enter two numbers");
scanf("%d%d",&a,&b);
printf("Values before swapping a=%d b=%d\n",a,b);
swap(&a,&b);
printf("Values after swapping a=%d b=%d",a,b);
}
o/p:Enter two numbers20
30
Values before swapping a=20 b=30
Values after swapping a=30 b=20

